{
  "version": 2,
  "name": "Film_FE",
  "builds": [
    { "src": "Film_FE/**", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Film_FE/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/Film_FE/$1"
    }
  ]
}
