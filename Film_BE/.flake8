[flake8]
max-line-length = 120
extend-ignore = E203, W503, E501
exclude =
    .git,
    __pycache__,
    venv,
    .venv,
    migrations,
    manage.py,
    settings.py,
    */migrations/*,
    */venv/*,
    */__pycache__/*
per-file-ignores =
    __init__.py:F401
    migrations/*:F401
    settings.py:F401,F403
